test() {
	if [ -z "$1" ]; then
		../push_swap $1 >out 2>out_err
		[ -z "$(cat out)" -a -z "$(cat out_err)" ] && echo OK || echo KO
		rm out
	else
		../push_swap $1 >out
		./checker $1 <out >out_checker
		num_moves=$(wc -l out | awk '{print $1}')
		[ $(cat out_checker) = "OK" ] && echo OK \($num_moves\) || echo KO
		rm out
		rm out_checker
	fi
}

test "1 2 3 4 5"
test "1 2 3 5 4"
test "1 2 4 3 5"
test "1 2 4 5 3"
